@page "/portfolio"

<main role="main">

      <section class="jumbotron text-center">
        <div class="container">
          <h1 class="jumbotron-heading">Portfolio</h1>
          <p class="lead text-muted">My past projects both professional and personal. Feel free to check them out, and check back often as I will be updating this page as more projects become available</p>
          
        </div>
      </section>

      <div class="album py-5 ">
        <div class="container">

          <div class="row">
            <div class="col-md-4">
              <div class="card mb-4 box-shadow">
                <img class="card-img-top" data-src="holder.js/100px225?theme=thumb&amp;bg=55595c&amp;fg=eceeef&amp;text=Thumbnail" alt="Thumbnail [100%x225]" src="Assets\Images\MikePageSnippet.png" data-holder-rendered="true" style="height: 225px; width: 100%; display: block;">
                <div class="card-body">
                  <p class="card-text">A simple landing page for Dr. Michael Mcdowell. Made using only HTML and CSS.</p>
                  <div class="d-flex justify-content-between align-items-center">
                    <div class="btn-group">
                      <button type="button" class="btn btn-sm btn-primary">Check it out</button>
                    </div>
                    <small class="text-muted">HTML-CSS</small>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-md-4">
              <div class="card mb-4 box-shadow">
                <img class="card-img-top" data-src="holder.js/100px225?theme=thumb&amp;bg=55595c&amp;fg=eceeef&amp;text=Thumbnail" alt="Thumbnail [100%x225]" src="https://opengraph.githubassets.com/295dfc0e787502102c17728393a1e850f2380e7e8ebb575dab38ba404d397729/coding812/CSharpPlayersGuide" data-holder-rendered="true" style="height: 225px; width: 100%; display: block;">
                <div class="card-body">
                  <p class="card-text">My solutions for the challenges set forth in The C# Player's Guide, 5th Edition</p>
                  <div class="d-flex justify-content-between align-items-center">
                    <div class="btn-group">
                      <button type="button" class="btn btn-sm btn-primary">Check it out</button>
                    </div>
                    <small class="text-muted">C#-.NET CLI</small>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-md-4">
              <div class="card mb-4 box-shadow">
